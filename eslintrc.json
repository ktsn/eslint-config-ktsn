{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "prettier"
  ],
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "no-cond-assign": 2,
    "no-constant-condition": 1,
    "no-control-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 1,
    "no-empty": 1,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-func-assign": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "array-callback-return": 1,
    "default-case": 1,
    "eqeqeq": [2, "always", { "null": "ignore" }],
    "no-caller": 2,
    "no-div-regex": 2,
    "no-empty-pattern": 2,
    "no-eval": 2,
    "no-floating-decimal": 2,
    "no-global-assign": 2,
    "no-implied-eval": 2,
    "no-octal": 1,
    "no-redeclare": 2,
    "no-script-url": 2,
    "no-self-assign": 2,
    "no-unused-expressions": 2,
    "no-unused-labels": 2,
    "no-useless-return": 2,
    "no-delete-var": 2,
    "no-undef": 2,
    "no-unused-vars": 1,
    "no-lonely-if": 2,
    "constructor-super": 2,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-imports": 2,
    "no-new-symbol": 2,
    "no-this-before-super": 2,
    "no-useless-computed-key": 2,
    "no-var": 2,
    "object-shorthand": 2,
    "prefer-const": 2,
    "require-yield": 2,
    "prettier/prettier": 2
  }
}
